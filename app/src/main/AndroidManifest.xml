<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jackknife">

    <application
        android:name="com.lwh.jackknife.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 不配置GlobalConfig，默认就是使用的DefaultGlobalConfig，你也可以自己实现GlobalConfig接口，
        然后添加你自定义GlobalConfig的class配置 -->
<!--        <meta-data android:name="com.lwh.jackknife.DefaultGlobalConfig" android:value="GlobalConfig"/>-->
    </application>

</manifest>